{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@/*": ["src/*"],
      "@/componentes/*": ["src/components/*"],
      "@/assets/*": ["src/assets/*"],
      "@/icons/*": ["src/icons/*"],
      "@/helpers/*": ["src/helpers/*"],
      "@/utils/*": ["src/utils/*"]
    }
  }
}
